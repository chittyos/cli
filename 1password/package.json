    "@types/compression": "^1.8.1",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/jest": "^29.5.12",
    "@types/multer": "^2.0.0",
    "@types/node": "^20.11.24",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^7.1.0",
    "@typescript-eslint/parser": "^7.1.0",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "eslint": "^8.57.0",
    "express-rate-limit": "^8.1.0",
    "helmet": "^8.1.0",
    "jest": "^29.7.0",
    "multer": "^2.0.2",
    "supertest": "^6.3.4",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "ts-jest": "^29.1.2",
    "typescript": "^5.3.3",
    "winston": "^3.17.0"
  },
  "dependencies": {
    "dotenv": "^16.4.5",
    "express": "^4.18.3"
  }
